{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "flynsarmy/csv-seeder": "1.*",
        "iio/libmergepdf": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/scout": "^5.0",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^5.3",
        "owen-it/laravel-auditing": "^8.0",
        "spatie/eloquent-sortable": "^3.4",
        "spatie/laravel-activitylog": "^2.8",
        "spatie/laravel-backup": "^5.10",
        "spatie/laravel-event-projector": "^1.0.0",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^2.17",
        "spatie/laravel-query-builder": "^1.10",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-tags": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "facade/ignition": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Directoryxx\\Finac\\": "packages/directoryxx/finac/src/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
